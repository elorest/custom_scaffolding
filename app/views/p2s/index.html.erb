<p id="notice"><%= notice %></p>

<h1>Listing to you: P2s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @p2s.each do |p2| %>
      <tr>
        <td><%= p2.title %></td>
        <td><%= link_to 'Show', p2 %></td>
        <td><%= link_to 'Edit', edit_p2_path(p2) %></td>
        <td><%= link_to 'Destroy', p2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New P2', new_p2_path %>
