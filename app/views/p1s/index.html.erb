<p id="notice"><%= notice %></p>

<h1>Listing P1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @p1s.each do |p1| %>
      <tr>
        <td><%= p1.title %></td>
        <td><%= link_to 'Show', p1 %></td>
        <td><%= link_to 'Edit', edit_p1_path(p1) %></td>
        <td><%= link_to 'Destroy', p1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New P1', new_p1_path %>
